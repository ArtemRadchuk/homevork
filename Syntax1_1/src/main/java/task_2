public class Main {
    public static void main(String[] args) {
        int vasyaAge = 10;
        int katyaAge = 20;
        int mishaAge = 30;
        int minimalage = -1;
        int middleage = -1;
        int maxage = -1;

        //Ищем кто старше
        if (vasyaAge > katyaAge& vasyaAge > mishaAge){
            maxage = vasyaAge;}
        else if(katyaAge > vasyaAge & katyaAge > mishaAge){
            maxage = katyaAge;
        } else if (mishaAge > katyaAge & mishaAge > vasyaAge){
            maxage = mishaAge;}

        //Ищем кто младше
        if (vasyaAge < katyaAge& vasyaAge < mishaAge){
            minimalage = vasyaAge;}
        else if(katyaAge < vasyaAge & katyaAge < mishaAge){
            minimalage = katyaAge;
        } else if (mishaAge < katyaAge & mishaAge < vasyaAge){
            minimalage = mishaAge;}

        //Ищем среднего
        if (katyaAge < mishaAge | vasyaAge & katyaAge > mishaAge | vasyaAge) {
            middleage = katyaAge}
        else  if (mishaAge < katyaAge | vasyaAge & mishaAge > katyaAge | vasyaAge) {
            middleage = mishaAge}
        else  if (vasyaAge < mishaAge | katyaAge & vasyaAge > mishaAge | katyaAge) {
            middleage = vasyaAge}
        System.out.println(minimalage "- Возраст младшего");
        System.out.println(middleage "- Возраст среднего");
        System.out.println(maxage "- Возраст старшего");
    }
}